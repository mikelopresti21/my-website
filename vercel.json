{
    "builds": [
      { "src": "server.js", "use": "@vercel/node" },
      { "src": "dist/mikes-website/browser", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/server.js" }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ]
}